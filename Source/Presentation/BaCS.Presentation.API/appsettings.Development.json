{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "PostgresOptions": {
        "Username": "user",
        "Password": "password",
        "Port": 5432,
        "Host": "localhost",
        "Database": "postgres",
        "Schema": "public",
        "AdditionalProperties": "Include Error Detail=true;"
    },
    "TracingOptions": {
        "ServiceName": "bacs-api-dev",
        "OtlpCollectorUrl": "http://localhost:4317",
        "OtlpProtocol": 0
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console"
        ],
        "MinimumLevel": {
            "Default": "Debug"
        },
        "WriteTo": [
            {
                "Name": "Console"
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithExceptionDetails"
        ]
    }
}
